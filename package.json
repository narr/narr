{
  "name": "narr",
  "version": "1.0.0",
  "scripts": {
    "typings": "typings",
    "predev": "rimraf ./temp",
    "dev": "webpack-dev-server --config ./config/webpack.dev.js --inline --progress",
    "dev:hot": "npm run dev -- --hot",
    "dev:ip": "npm run dev -- --host 192.168.0.2 --port 8080",
    "dev:ip:hot": "npm run dev:ip -- --hot",
    "test": "karma start ./config/karma.conf.js --single-run",
    "test:watch": "karma start ./config/karma.conf.js",
    "_build": "webpack --config ./config/webpack.prod.js --progress --display-modules --bail",
    "build": "npm run _build && node ./config/sprite.js",
    "build:dev": "npm run _build -- -my-dev && node ./config/sprite.js -my-dev",
    "build:ghp": "npm run _build -- -my-ghp && node ./config/sprite.js -my-ghp",
    "server": "http-server ./dist",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor ./config/protractor.chrome.js",
    "e2e:ip": "npm run e2e -- --baseUrl http://192.168.0.2:8080/",
    "e2e:ip:live": "npm run e2e:ip -- --elementExplorer",
    "doc": "typedoc --options ./typedoc.json ./src",
    "start": "npm run dev",
    "postinstall": "typings install"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "angular2-hmr": "^0.7.0",
    "autoprefixer": "^6.3.6",
    "codelyzer": "0.0.23",
    "copy": "^0.2.0",
    "core-js": "^2.4.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.3",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-spec-reporter": "^2.5.0",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-remap-istanbul": "^0.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.2",
    "rxjs": "5.0.0-beta.6",
    "sass-loader": "^3.2.0",
    "sockjs-client": "1.0.3",
    "sprite-webpack-plugin": "git+https://github.com/narr/sprite-webpack-plugin.git",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.9.1",
    "tslint": "^3.10.2",
    "tslint-loader": "^2.1.4",
    "typedoc": "^0.4.3",
    "typescript": "^1.8.10",
    "typings": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "zone.js": "^0.6.12"
  },
  "engines": {
    "node": ">= 5.10"
  },
  "private": true
}
